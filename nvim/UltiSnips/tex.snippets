snippet frame "frame" b
\begin{frame}{$1}
  $0
\end{frame}

endsnippet

snippet eq "equation" b
\begin{equation}\label{eq:$1}
	$1
\end{equation}
$0
endsnippet

snippet eq* "equation*" b
\begin{equation*}
	$1
\end{equation*}
$0
endsnippet



snippet eql "equation inline" w
\$ $1 \$ $0
endsnippet

snippet beg "begin" b
\begin{$1}
	$0
\end{$1}

endsnippet


snippet { "{ }" w
{ $1 } $0
endsnippet

snippet sec "section" b
\section{ $1 }
$0
endsnippet
